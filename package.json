{
  "name": "corona-travel_client",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=16.8.0",
    "pnpm": ">=6.16.1"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "run-s lint:*",
    "lint:next": "next lint",
    "lint:prettier": "npx prettier --ignore-path ./.gitignore --check .",
    "lint:stylelint": "stylelint '**/*.css'",
    "fix": "run-p fix:*",
    "fix:next": "next lint --fix",
    "fix:prettier": "npx prettier --ignore-path ./.gitignore --write .",
    "fix:stylelint": "stylelint --fix '**/*.css'",
    "stylelint-config-prettier-check": "stylelint-config-prettier-check"
  },
  "dependencies": {
    "next": "11.1.2",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@types/react": "17.0.27",
    "autoprefixer": "^10.3.7",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.9",
    "prettier": "2.4.1",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^9.0.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-prettier": "^1.2.0",
    "tailwindcss": "^2.2.16",
    "typescript": "4.4.3"
  }
}
